function a13()
  A11 = gg.multiChoice({
"🔎ɢᴏ́ᴄ ɴʜɪ̀ɴ ɢᴀ̂̀ɴ",
"🔎ɢᴏ́ᴄ ɴʜɪ̀ɴ x5",
"🔎ɢᴏ́ᴄ ɴʜɪ̀ɴ x10",
"🔎ɢᴏ́ᴄ ɴʜɪ̀ɴ ᴋɪᴇ̂̉ᴜ 1",
"🔎ɢᴏ́ᴄ ɴʜɪ̀ɴ ᴋɪᴇ̂̉ᴜ 2",
"🔎ɢᴏ́ᴄ ɴʜɪ̀ɴ ᴋɪᴇ̂̉ᴜ 3",
"➡️ᴠᴇ̂̀ ᴍᴇɴᴜ"
 }, nil, "⚠️ǫᴜᴀʏ ᴄʟɪᴘ ʜɪɢʜᴛʟɪɢʜᴛ ᴄᴜ̛̣ᴄ ᴄʜᴀ̂́ᴛ⚠️")
  if SN == nil then
  else
  if A11[1] == true then
   z1()
  end
  if A11[2] == true then
   z2()
  end
  if A11[3] == true then
   z2z()
  end
  if A11[4] == true then
   z3()
  end
  if A11[5] == true then
   z4()
  end
  if A11[6] == true then
   z5()
  end
  if A11[7] == true then
   Main()
  end
end
  XGCK = -1
end

function z1()
gg.clearResults()
	 gg.setRanges(32)
	 gg.searchNumber("4.09999990463", gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1)
	 gg.getResults(10)
	 gg.editAll("1", gg.TYPE_FLOAT)
	 gg.clearResults()
end

