function Main()
  SN = gg.multiChoice({
"彡sᴘᴇᴇᴅ ᴘᴄ",
"彡sᴘᴇᴇᴅ ᴀɴᴅʀᴏɪᴅ",
"彡ᴄᴡᴡ ",
"彡ᴅʀɪғᴛ sᴘᴇᴇᴅ",
"彡ɴɪᴛʀᴏ x 2",
"彡ɴɪᴛʀᴏ x ᴍᴀx",
"彡ᴅʀɪғᴛ ɴᴏ sʟᴏᴡ",
"彡ʟᴏᴄᴋ sᴘᴇᴇᴅ",
"╭═ ᴇxɪᴛ ═╮"
 }, nil,'╭═════════════════╮\n ➖ʜᴀᴄᴋ ᴢɪɴɢ sᴘᴇᴇᴅ ᴍᴏʙɪʟᴇ➖ \n  ʏᴏᴜᴛᴜʙᴇ: ᴛᴇᴀᴄʜᴇʀ 4 ɢᴀᴍɪɴɢ\n      ➖➖🔹ᴍᴜ̀ᴀ 4🔹➖➖\n╰═════════════════╯ ')
if SN == nil then else
if SN[1] == true then a1() end
if SN[2] == true then a2() end
if SN[3] == true then a3() end
if SN[4] == true then a4() end
if SN[5] == true then a5() end
if SN[6] == true then a6() end
if SN[7] == true then a7() end
if SN[8] == true then a8() end
if SN[9] == true then Exit() end
end
  XGCK = -1
end

function a1()
gg.clearResults()
gg.setRanges (32)
gg.searchNumber('0.27777779102F;1000.0F;0.0001F;-1.0F;1.0F', gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1)
gg.searchNumber('0.27777779102', gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1)
gg.getResults(20)
gg.editAll("0.35", gg.TYPE_FLOAT)
gg.clearResults()
end
function a2()
gg.clearResults()
gg.setRanges(4)
gg.searchNumber("0.27777779102;128D;352D;0D;0F::450",  gg.TYPE_FLOAT,false,gg.SIGN_EQUAL,0, -1)
gg.searchNumber("0", gg.TYPE_FLOAT,false,gg.SIGN_EQUAL,0,-1)
gg.getResults(3)
gg.editAll("35",gg.TYPE_FLOAT)
gg.clearResults()
gg.clearResults()
gg.setRanges (4)
gg.searchNumber('0.27777779102F;1000.0F;0.0001F;-1.0F;1.0F', gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1)
gg.searchNumber('0.27777779102', gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1)
gg.getResults(2, 7)
gg.editAll("0.32", gg.TYPE_FLOAT)
gg.clearResults()
end

function a3()
gg.clearResults()
gg.setRanges(32)
gg.searchNumber("35;0.5;0.6;1.399;50.00000381472", 16, false, 536870912, 0, -1)
gg.searchNumber("35", 16, false, 536870912, 0, -1)
gg.getResults(1000)
gg.editAll("0", 16)
gg.clearResults()
end

