function Exit()
print("╭═════════════════╮\nYᴏᴜTᴜʙᴇ : Tᴇᴀᴄʜᴇʀ 4 Gᴀᴍɪɴɢ\n╰═════════════════╯ ")
os.exit()
gg.setVisible(true)
end



while true do
  if gg.isVisible(true) then
    XGCK = 1
    gg.setVisible(false)
  end
  gg.clearResults()
  if XGCK == 1 then
    Main()
  end
end
